{
	"name": "default",
    "shell": "zsh",
    "shell_framework": "oh-my-zsh",
    "shell_theme": "spaceship-prompt",
	"shell_plugins": [
        {"name": "git"},
        {"name": "tmux"},
        {
            "name": "zsh-autosuggestions",
            "install": true,
            "github_url": "https://github.com/zsh-users/zsh-autosuggestions.git"
        },
        {
            "name": "zsh-syntax-highlighting",
            "install": true,
            "github_url": "https://github.com/zsh-users/zsh-syntax-highlighting.git"
        }
	],
	"packages": [
        {"name": "vim"},
        {"name": "curl"},
        {"name": "wget"},
        {"name": "grc"},
        {"name": "htop"}
    ],
    "custom_install": []
}