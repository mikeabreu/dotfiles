{
	"name": "macOS",
    "shell": "zsh",
    "shell_framework": "oh-my-zsh",
    "shell_theme": "spaceship-prompt",
	"shell_plugins_no_installation": [
        "git",
        "aws",
        "docker",
        "docker-compose",
        "docker-machine",
        "terraform",
        "tmux",
        "vscode",
        "python",
        "virtualenv",
        "nmap",
        "osx"
    ],
    "shell_plugins_to_install": [
        "zsh-autosuggestions",
        "zsh-syntax-highlighting"
    ],
	"system_packages": [
        "vim",
        "git",
        "curl",
        "wget",
        "grc",
        "htop"
    ],
    "configs_home": [
        "zsh/.zshrc_default",
        "iterm2/main.json",
        "ssh/ssh_config",
        "vim/.vimrc_default",
        "vscode/settings.json"
    ],
    "configs_etc": [
        "ssh/sshd_config"
    ],
    "configs_etc_symlink": true,
    "tools": []
}