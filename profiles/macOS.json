{
	"name": "macOS",
    "shell": "zsh",
    "shell_framework": "oh-my-zsh",
    "shell_theme": "spaceship-prompt",
	"shell_plugins_no_installation": [
        "git",
        "aws",
        "docker",
        "docker-compose",
        "docker-machine",
        "terraform",
        "tmux",
        "vscode",
        "python",
        "virtualenv",
        "nmap",
        "osx"
    ],
    "shell_plugins_to_install": [
        "zsh-autosuggestions",
        "zsh-syntax-highlighting"
    ],
	"system_packages": [
        "vim",
        "git",
        "curl",
        "wget",
        "grc",
        "htop",
        "bat"
    ],
    "configs_home": [
        "macos=.zshrc"
    ]
}